{
  "manifest_version": 3,
  "name": "Variational WS Proxy",
  "version": "1.0",
  "description": "保存原生WebSocket并注入代理脚本",
  "permissions": [],
  "host_permissions": [
    "https://omni.variational.io/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://omni.variational.io/*"],
      "js": ["inject.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}
